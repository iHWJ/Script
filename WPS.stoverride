name: WPS Office
desc: 解锁超级会员部分功能

http:
  mitm:
    - "account.wps.cn"

  script:
    - match: ^https?:\/\/account\.wps\.cn\/api\/users
      name: WPS
      type: response
      require-body: true
      timeout: 30

script-providers:
  WPS:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Wps.js
    interval: 3600
