name: 微信公众号去广告
desc: 去除微信公众号广告

http:
  mitm:
    - "mp.weixin.qq.com"

  script:
    - match: ^https?:\/\/mp\.weixin\.qq\.com\/mp\/getappmsgad
      name: Wechat
      type: response
      require-body: true
      timeout: 30

script-providers:
  Wechat:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/QuantumultX/File/Wechat.js
    interval: 3600
