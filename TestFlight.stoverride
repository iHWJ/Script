name: TestFlight区域限制解除
desc: 适用于更新TestFlight App时, 提示"APP不可用"问题.

http:
  mitm:
    - "testflight.apple.com"
  script:
    - match: ^https?:\/\/testflight\.apple\.com\/v\d\/accounts\/.+?\/install$
      name: TestFlight
      type: request
      require-body: true
      timeout: 10
      
script-providers:
  TestFlight:
    url: https://github.com/iHWJ/script/raw/main/js/TF_Download.js
    interval: 86400
